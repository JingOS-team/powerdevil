<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE kcfg SYSTEM "http://www.kde.org/standards/kcfg/1.0/kcfg.dtd">
<kcfg>
  <kcfgfile name="powerdevilrc"/>

  <group name="GUI">
    <entry name="RootWarning" type="Bool">
      <default>false</default>
    </entry>
  </group>
  <group name="Plugins">
    <entry name="PlugActivated" type="StringList">
      <default>General,Ramdisk,Network</default>
    </entry>
  </group>
</kcfg>
